/** app.wxss **/

.container {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
    font-size: 28rpx;
    background: #f5f5f5;
    box-sizing: border-box;
    overflow: hidden;
}

/** 隐藏滚动条 **/
::-webkit-scrollbar {
    width: 0;
    height: 0;
    background: transparent;
}
